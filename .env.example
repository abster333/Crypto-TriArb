# Copy to .env and fill with your endpoints/ports. Unused keys are ignored by v5 Settings.

# Shared infra
NATS_URL=nats://127.0.0.1:4222
REDIS_URL=redis://127.0.0.1:6379/0

# cex_triarb_v1 (optional)
EXCHANGES=COINBASE,KRAKEN
SYMBOLS=BTCUSD,ETHUSD,ETHBTC
UI_PORT=8086
HEALTH_PORT=8085
DEPTH_ENABLED=false            # set true to publish depth snapshots
DEPTH_LEVELS=10                # depth levels to keep/publish
CB_WS_CHANNELS=ticker,level2   # override Coinbase WS channels (level2 required for depth)
KRAKEN_BOOK_DEPTH=25           # Kraken book subscription depth
DEPTH_OKX_BOOK_DEPTH=10        # OKX books depth (books5 top levels)
OKX_QUOTES=USD,USDT,USDC,BTC,ETH

# v5 DEX stack
ETHEREUM_RPC_URL=https://your-mainnet-http-rpc
ETHEREUM_WS_URL=wss://your-mainnet-ws-rpc
BSC_RPC_URL=https://your-bsc-http-rpc
BSC_WS_URL=wss://your-bsc-ws-rpc
RPC_MAX_QPS=8                # batch RPC calls per second (polling/resync)
RPC_MAX_BATCH_SIZE=20        # JSON-RPC batch size
METRICS_PORT=9100
WS_SUB_BATCH_SIZE=30          # how many pools per WS subscribe batch
V5_GEN_MANIFEST=false         # set true to regenerate manifests if missing
RESYNC_INTERVAL_SECONDS=600
ENABLE_POLL_FALLBACK=false
V5_TOP_POOLS=50               # number of top pools per exchange when generating manifests
COINGECKO_API_KEY=your-coingecko-api-key
COINGECKO_TIER=demo          # demo â‰ˆ30 req/min; use 'paid' if available

# Strategy thresholds (optional)
STRATEGY_TRI_THRESHOLD_BPS=10.0
STRATEGY_INITIAL_AMOUNT=1.0    # base notional in starting token for ROI simulation
STRAT_USE_DEPTH=false          # set true for depth-aware tri-arb

# Manifest generation targets (optional, per-exchange)
# Format: network:dex:top_count,network:dex:top_count
V5_TARGETS=eth:uniswap_v3:50,bsc:pancakeswap-v3-bsc:50
